{
    "name": "dedermus/api-tester",
    "description": "Api tester for open-admin",
    "type": "library",
    "keywords": ["open-admin", "api", "tester"],
    "homepage": "https://github.com/dedermus/api-tester",
    "license": "MIT",
    "authors": [
        {
            "name": "dedermus",
            "email": "dedermus@yandex.ru"
        }
    ],
    "require": {
        "php": "~7.3|~8.0",
        "dedermus/open-admin-core": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "OpenAdminCore\\Admin\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdminCore\\Admin\\ApiTester\\ApiTesterServiceProvider"
            ]
        }
    }
}
